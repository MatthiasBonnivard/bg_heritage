using UnityEngine;
using System.Collections;

public class s_input_pep_onatha : MonoBehaviour {

	// Edit > Project Settings > Input
	// Name			Breath
	// Gravity		0
	// Dead			0
	// Sensitivity	1
	// Type 		Joystick Axis
	// Axis			3rd axis
	// Joy Num		Get motion from all

	void Start () {
	}

	void Update () {
		s_input.pressure = Input.GetAxisRaw("Breath");
	}
}
